typedef struct {

    char prop[30];
    char comb[15];
    char modelo[25];
    char cor[18];
    int chassi;
    int ano;
    char placa[10];

} Veiculos;

int main() {

	int i;
	Veiculos veic;
	Veiculos vetor[20];

    for (i=0;i<TAM;i++) {
        printf("Digite o proprietario do cacrro.\n");
        fgets(veic.prop,30,stdin);
        //tuts(veic.prop);

        printf("Digite o tipo de combustivel do carro\n");
        fgets(veic.comb,10,stdin);
        //tuts(veic.comb);

        printf("Digite o modelo do veiculo\n");
        fgets(veic.modelo,22,stdin);
        //tuts(veic.modelo);

        printf("Digite a cor do veiculo\n");
        fgets(veic.cor,28,stdin);

        printf("Digite o nÃºmero de chassi do veiculo\n");
        scanf("%d", &veic.chassi);

        printf("Digite o ano do veiculo\n");
        scanf("%d", &veic.ano);

        printf("Digite a placa do veiculo\n");
        fgets(veic.placa,10,stdin);

        vetor[i] = veic;
    }

    return 0;
}
