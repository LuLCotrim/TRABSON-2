typedef struct {

    char prop[30];
    char comb[15];
    char modelo[25];
    char cor[18];
    int chassi;
    int ano;
    char placa[10];

}veiculos;

int main() {

	int cont=0;
	veiculos veic[20];

    while (cont < TAM) {
        printf("Digite o proprietario do cacrro.\n");
        fgets(veic.prop[cont],30,stdin);
        //tuts(veic.prop);

        printf("Digite o tipo de combustivel do carro\n");
        fgets(veic.comb[cont],10,stdin);
        //tuts(veic.comb);

        printf("Digite o modelo do veiculo\n");
        fgets(veic.modelo[cont],22,stdin);
        //tuts(veic.modelo);

        printf("Digite a cor do veiculo\n");
        fgets(veic.cor[cont],28,stdin);

        printf("Digite o nÃºmero de chassi do veiculo\n");
        scanf("%d", &veic.chassi[cont]);

        printf("Digite o ano do veiculo\n");
        scanf("%d", &veic.ano[cont]);

        printf("Digite a palca do veiculo\n");
        fgets(veic.placa[cont],10,stdin);

        cont++;
    }

    return 0;
}
